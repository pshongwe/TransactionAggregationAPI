{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Error"
    },
    "Console": {
      "IncludeScopes": false,
      "TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffZ"
    }
  },
  "AllowedHosts": "localhost;127.0.0.1",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  }
}
